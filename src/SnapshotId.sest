import Models

module SnapshotId = struct
  open Stdlib

  type t = Models.snapshot_id

  val generate<$a> : fun() -> [$a]t = external 0 ```
    generate() ->
        Uuid = uuid:get_v4(),
        list_to_binary(uuid:uuid_to_string(Uuid)).
  ```

  val equal(s1 : t, s2 : t) : bool =
    Binary.equal(s1, s2)

end
